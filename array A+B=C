#include <stdio.h>

   int main()
{
    int a[2][3]={{6,9,8},{4,1,2}};
    int b[2][3]={{1,5,8},{9,4,3}};
    int c00=a[0][0]+b[0][0];
    int c01=a[0][1]+b[0][1];
    int c02=a[0][2]+b[0][2];
    int c10=a[1][0]+b[1][0];
    int c11=a[1][1]+b[1][1];
    int c12=a[1][2]+b[1][2];
    int ans=c00+c01+c02+c10+c11+c12;
    printf("A[2][3]+B[2][3]== C[2][3]={{%d,%d,%d},{%d,%d,%d}};",c00,c01,c02,c10,c11,c12);


    return 0;
}
